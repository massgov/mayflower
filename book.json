{
  "language": "en",
  "root": "docs/",
  "author": "Minghua Sun",
  "title": "Mayflower Documentation",
  "plugins": ["edit-link", "github", "header", "-livereload", "simpletabs", "github-embed", "hints"],
  "pluginsConfig": {
    "layout": {
      "headerPath" : "header.html"
    },
    "lunr": {
      "ignoreSpecialCharacters": true
    },
    "edit-link": {
      "base": "https://massds.gitbook.io/mayflower/~/edit/primary/",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/massgov/mayflower"
    },
    "github-embed": {
      "showLink": true,
      "reindent": true
    }
  }
}
