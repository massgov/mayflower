{% block header %}{% endblock %}
<main id="main-content" {% if flexibleLinkGroup %} class="ma__services ma__services-flexible-link-groups" {% else %} class="ma__services" {% endif %} tabindex="-1" {% if schemaPageType %} typeof="{{schemaPageType}}" {% endif %}>
	<div class="pre-content">
		{% block preContent %}
			{% if breadcrumbs %}
				{% include "@organisms/by-template/breadcrumbs.twig" %}
			{% endif %}
			{% include "@molecules/relationship-indicators.twig" %}
			{% include "@organisms/page-banner-legacy/page-banner-legacy.twig" %}
		{% endblock %}
	</div>
	<div class="page-content--intro">
		{% if introSidebar.logo %}
			{% set image = introSidebar.logo %}
			{% include "@atoms/09-media/image.twig" %}
		{% endif %}
		{% block pageContentIntro %}

			{% set richText = introPageContent.intro %}
			{% include "@organisms/by-author/rich-text.twig" %}

			{% if introPageContent.video %}
				{% set video = introPageContent.video %}
				{% include "@atoms/09-media/video.twig" %}
			{% endif %}
		{% endblock %}
	</div>
	<div class="stacked-row-content">
		<section class="ma__stacked-row__section">
			<div class="main-content main-content--full">
				<div class="page-content">
					{% if flexibleLinkGroup is not defined or flexibleLinkGroup is null or flexibleLinkGroup is same as(false) %}
						{% if introPageContent.keyActions %}
							{% set keyActions = introPageContent.keyActions %}
							{% include "@organisms/by-author/key-actions.twig" %}
						{% endif %}
					{% endif %}
					{% block pageContent %}
						{% if doActionFinder %}
							{% set actionFinder = doActionFinder.actionFinder %}
							{% include "@organisms/by-author/action-finder.twig" %}
						{% endif %}
						{% if flexibleLinkGroup is not defined or flexibleLinkGroup is null or flexibleLinkGroup is same as(false) %}
							{% if learnActionFinder %}
								{% set actionFinder = learnActionFinder.actionFinder %}
								{% include "@organisms/by-author/action-finder.twig" %}
							{% endif %}
						{% endif %}
						{% if contactList %}
							{% include "@organisms/by-author/contact-list.twig" %}
						{% endif %}
						{% if mappedLocations %}
							{% include "@organisms/by-author/mapped-locations.twig" %}
						{% endif %}
						{% if flexibleLinkGroup is not defined or flexibleLinkGroup is null or flexibleLinkGroup is same as(false) %}
							{% if eventListing %}
								{% include "@organisms/by-author/event-listing.twig" %}
							{% endif %}
						{% endif %}
						{% if flexibleLinkGroup is not defined or flexibleLinkGroup is null or flexibleLinkGroup is same as(false) %}
							{% if splitColumns %}
								<div class="more-info-header">
									<h2 class="ma__comp-heading">More Information</h2>
								</div>
								{% include "@organisms/by-author/split-columns.twig" %}
							{% endif %}
						{% endif %}
						{% if flexibleLinkGroup %}
							{% if splitColumns %}
								<div class="more-info-header">
									<h2 class="ma__comp-heading">Related Services</h2>
								</div>
								{% include "@organisms/by-author/split-columns.twig" %}
							{% endif %}
						{% endif %}
					{% endblock %}
				</div>
			</div>
		</section>

	</div>
	{% block schemaContent %}{% endblock %}
</main>

{% block footer %}{% endblock %}
