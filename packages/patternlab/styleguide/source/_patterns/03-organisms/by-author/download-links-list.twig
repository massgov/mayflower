<section class="ma__downloads-listing">
  {% if downloadList.compHeading %}
    {% set compHeading = downloadList.compHeading %}
    {% include "@atoms/04-headings/comp-heading.twig" %}
  {% endif %}
  {% if downloadList.sidebarHeading %}
    {% set sidebarHeading = downloadList.sidebarHeading %}
    {% include "@atoms/04-headings/sidebar-heading.twig" %}
  {% endif %}
  <ul class="ma__downloads-listing__links">
    {% block downloadListLinks %}
      {% for item in downloadList.downloadLinks %}
        {% set downloadLink = item.downloadLink %}
        {% include "@molecules/download-link-multilang.twig" %}
      {% endfor %}
    {% endblock %}
  </ul>
</section>
