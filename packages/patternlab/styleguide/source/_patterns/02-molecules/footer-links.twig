<nav class="ma__footer-links" role="navigation" aria-label="Footer navigation">
{% for item in footerLinks.items %}
  <ul class="ma__footer-links__items" aria-label="{{ item.heading }}">
    {% for link in item.links %}
      <li class="ma__footer-links__item">
        <a href="{{ link.href }}" class="ma__footer-links__link">{{ link.text }}</a>
      </li>
    {% endfor %}
  </ul>
{% endfor %}
</nav>
