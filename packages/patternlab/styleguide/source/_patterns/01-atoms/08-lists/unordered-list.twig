<ul{{ class ? ' class="' ~ class ~ '"' }}>
	{% for listItem in unorderedList %}
		<li>{{ listItem.text }}</li>
		{% if listItem.sublist %}
		<li>{{ listItem.text }}
			<ul>{# 2nd level #}
				{% for sublist in listItem.sublist %}
					<li>{{ sublist.text }}
					{% if sublist.sublist %}
						<ul>{# 3rd level #}
							{% for sublist in sublist.sublist %}
							<li>{{ sublist.text }}
							{% if sublist.sublist %}
								<ul>
									{# 3rd level #}
									{% for sublist in sublist.sublist %}
										<li>{{ sublist.text }}</li>
									{% endfor %}
								</ul>
							{% endif %}
							</li>
							{% endfor %}
						</ul>
					{% endif %}
					</li>
				{% endfor %}
			</ul>
		</li>
		{% endif %}
	{% endfor %}
</ul>
