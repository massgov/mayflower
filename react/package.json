{
  "name": "@massds/mayflower-react",
  "description": "React versions of Mayflower design system UI components",
  "author": "Massachusetts Digital Services (MDS)",
  "version": "9.3.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib"
  ],
  "scripts": {
    "backstop:test": "docker-compose run backstop test",
    "backstop": "npm-run-all build-storybook backstop:test",
    "backstop:approve": "docker-compose run backstop approve",
    "build-css": "node-sass-chokidar --include-path ./src/components/styles --include-path ../assets/scss src/ --include-path node_modules src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./src/components/styles --include-path ../assets/scss src/ --include-path node_modules src/ -o src/ --watch",
    "build-nwb": "nwb build-react-component --copy-files",
    "build": "npm-run-all build-css build-nwb",
    "clean": "nwb clean-module",
    "start": "npm run jest:watch-and-generate && npm-run-all -p watch-css storybook ",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .; exit 0",
    "jest": "cross-env-shell SKIP_PREFLIGHT_CHECK=true react-scripts test --setupTestFrameworkScriptFile=./tests/setupTests.js",
    "jest:generate-json": "cross-env-shell SKIP_PREFLIGHT_CHECK=true react-scripts test --json --no-cache --no-watch --outputFile=./tests/jest-test-results.json --setupTestFrameworkScriptFile=./tests/setupTests.js",
    "jest:watch-and-generate": "cross-env-shell SKIP_PREFLIGHT_CHECK=true react-scripts test --json --no-cache --watchAll --setupTestFrameworkScriptFile=./tests/setupTests.js --outputFile=./tests/jest-test-results.json --watchPathIgnorePatterns=jest-test-results|| true",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run jest:generate-json && build-storybook"
  },
  "pre-push": [
    "lint"
  ],
  "dependencies": {
    "airbnb-prop-types": "2.13.2",
    "autosuggest-highlight": "3.1.1",
    "b-jsonp": "1.2.0",
    "bourbon": "5.1.0",
    "bourbon-neat": "1.7.2",
    "classnames": "2.2.6",
    "fuse.js": "3.4.5",
    "is": "3.3.0",
    "moment": "2.21.0",
    "normalize-scss": "7.0.1",
    "numbro": "2.1.2",
    "object.entries": "1.0.4",
    "pikaday": "1.7.0",
    "react-autosuggest": "9.4.3",
    "react-autowhatever": "10.2.0",
    "react-character-counter": "1.0.1",
    "react-compound-slider": "0.19.2",
    "react-portal": "4.2.0",
    "react-scripts": "2.1.8",
    "react-select": "1.2.1",
    "react-table": "6.8.6",
    "react-transition-group": "2.4.0",
    "shortid": "2.2.8",
    "svg-sprite-loader": "4.1.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@storybook/addon-a11y": "5.0.11",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-info": "5.0.11",
    "@storybook/addon-jest": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addon-storysource": "5.0.11",
    "@storybook/addon-viewport": "5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/react": "5.0.11",
    "acorn": "6.0.7",
    "acorn-jsx": "5.0.1",
    "acorn-object-rest-spread": "1.1.0",
    "acorn-static-class-property-initializer": "1.0.0",
    "babel-eslint": "9.0.0",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-import-paths": "1.0.0",
    "cross-env": "5.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-alias": "1.1.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.0.1",
    "jest-enzyme": "7.0.2",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.3",
    "nwb": "0.23.x",
    "pre-push": "0.1.1",
    "prop-types": "15.7.1",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "homepage": "",
  "license": "MIT",
  "repository": "https://github.com/massgov/mayflower/react",
  "keywords": [
    "react-component"
  ]
}
